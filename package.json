{
  "name": "bun-ui",
  "version": "1.0.0",
  "description": "a binding layer to GLFW and a plotting library",
  "main": "./lib/index.mjs",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "node buildd.mjs mkbuild && cd build && cmake .. && node ../buildd.mjs",
    "build-node": "node buildd.mjs delbuild && cmake-js build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liz3/bun-ui.git"
  },
