  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liz3/bun-ui/issues"
  },
  "homepage": "https://github.com/liz3/bun-ui#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "canvas": "github:Automattic/node-canvas",
    "node-addon-api": "^7.0.0",
    "cmake-js": "^7.2.1"
  }
}
